[Unit]
Description=ORCM Server Daemon
After=network.target

[Service]
EnvironmentFile=-/etc/sysconfig/orcmsched
ExecStart=/opt/openrcm/bin/orcmsched $ORCMSCHED_OPTS
KillMode=process
Restart=always

[Install]
WantedBy=multi-user.target
